<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="inventory_system"/>
  <table-filter match-name="user_addresses"/>
  <table-filter match-name="site"/>
  <table-filter match-name="job_titles"/>
  <table-filter match-name="credentials"/>
  <table-filter match-name="users"/>
  <table-filter match-name="ref_item_categories"/>
  <table-filter match-name="inventory_items"/>
  <table-filter match-name="user_assigned_to_site"/>
</hibernate-reverse-engineering>
